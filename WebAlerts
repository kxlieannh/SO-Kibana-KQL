#event 1125 - Connection to unsafe websites
event.code: "1125"
#using filters specify exactly what should be detcted 

#event 22 - DNS Request
event.code: "22" and winlog.event_data.QueryName: "*example.com*"
#winlog for specific sites ex tiktok.com instagram.com

#1126 - Raw IP Addresses
event.code: "1126" and winlog.event_data.DestinationIP: "*" and not winlog.event_data.DestinationHostname: "*"
#Contains no Domain name, possibly malicious
